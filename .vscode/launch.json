        {
            "name": "Python: Dynamic equivalent",
            "type": "python",
            "request": "launch",
            "program": "Scripts/DynamicEquivalents/DynamicEquivalent.py",
            "console": "integratedTerminal",
            "args": [
                "--working_dir",
                "/home/fsabot/Desktop/dynawo-algorithms/examples/CIGRE_MV_Wind/SA",
                "--fic_MULTIPLE",
                "fic_MULTIPLE.xml",
                "--reduced_fic_MULTIPLE",
                "reduced_fic.xml",
                "--name",
                "CIGRE_MV_Wind",
                "--reduced_name",
                "reduced",
                "--csv_par",
                "params_dyd.csv",
                "--csv_iidm",
                "params_iidm.csv",
                "--nb_runs_random",
                "300",
